<h1>Student Form</h1>
<%= form_tag students_path do %>
  <%= label_tag 'first_name', 'First Name: ' %>
  <%= text_field_tag :'student[first_name]', nil, id: 'first_name' %>
  <br>
  <%= label_tag 'last_name', 'Last Name: ' %>
  <%= text_field_tag :'student[last_name]', nil, id: 'last_name' %>
  <br><br>
  <%= submit_tag 'Submit Student' %>
<% end %>
